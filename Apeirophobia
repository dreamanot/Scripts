local LeftGroupBox = MainF:AddLeftGroupbox('Exits')

local x, y, z = -898.149231, 11.6797743, -92.4646378; -- Level 0 Exit
local x2, y2, z2 = -794.019958, -100.850189, -1076.2417; -- Level 1 Exit
local x3, y3, z3 = -582.800415, -176.937714, -2545.83789; -- Level 2 Exit
