local function Level0()
	Twn(Char:WaitForChild("HumanoidRootPart"), CFrame.new(x, y, z), 0.1);
	Twn(Char:WaitForChild("HumanoidRootPart"), CFrame.new(x, y, z), 0.1);
	Twn(Char:WaitForChild("HumanoidRootPart"), CFrame.new(x, y, z), 0.1);
end
local function Level1()
	Twn(Char:WaitForChild("HumanoidRootPart"), CFrame.new(x2, y2, z2), 0.1);
	Twn(Char:WaitForChild("HumanoidRootPart"), CFrame.new(x2, y2, z2), 0.1);
	Twn(Char:WaitForChild("HumanoidRootPart"), CFrame.new(x2, y2, z2), 0.1);
